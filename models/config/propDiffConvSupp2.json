{
    "data": {
        "preprocessors": [
            {"name": "ProportionalDiff", "args": ["close"]},
            {"name": "ProportionalDiff", "args": ["DJIA_close"]},
            {"name": "ProportionalDiff", "args": ["COMP_close"]}
        ]
    },
    "model": {
        "type": "NeuralNetwork",
        "layers": [
            {"type": "Conv1d", "args": [3, 3, 3]},
